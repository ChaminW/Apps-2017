<Script>
groupId='';
enquiryId='';
phone='';
name='';
	function fnLogin(){
		var str = $( "#login_form" ).serialize();
			$.post("techno/ajax.php?m=webLogin",
			str,
				function(data) {
					data=$.trim(data);  
					if(data.indexOf('ERROR')!=-1){
						alert('Invalid Phone No or Password. Please contact admin.');
						return;
					}else{
						phone=$('#cf_phone').val();
						groupId=data.split('#')[0];
						enquiryId=data.split('#')[1];
						name=data.split('#')[2];
						$('#detailsId').html("<h2>Welcome <B>"+name+"<B> Group Id : "+groupId+"</h2>");
						$('#contactId').hide();
						$('#MainMenu').show();
					}
				}
			);
	}
	function fnAddMeeting(){
		
		var str = $( "#meeting_form" ).serialize();
		str +='&groupId='+groupId+"&phone="+phone;
			$.post("techno/ajax.php?m=addWebMeeting",
			str,
				function(data) {
					data=$.trim(data);  
					
						alert(data);
					
				}
			);
	}
</Script>

